{
	"name": "DF_LoadEmployeeDIM",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "EmployeeCSV"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "NonNullEmployeeIDRecords"
				},
				{
					"name": "TypeConversionTimestamp"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpID as string,",
				"          Region as string,",
				"          Status as string,",
				"          Function as string,",
				"          Level as string,",
				"          Role as string,",
				"          StartDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmployeeCSV",
				"EmployeeCSV filter(!isNull(EmpID)) ~> NonNullEmployeeIDRecords",
				"NonNullEmployeeIDRecords derive(EmpID = toInteger(EmpID),",
				"          processtime = currentTimestamp()) ~> TypeConversionTimestamp"
			]
		}
	}
}