{
	"name": "DF_LoadEmployeeDIM",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "EmployeeCSV"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "DimEmp"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "NonNullEmployeeIDRecords"
				},
				{
					"name": "TypeConversionTimestamp"
				},
				{
					"name": "Typecpnversions"
				},
				{
					"name": "LkpTarget"
				},
				{
					"name": "selectnames"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpID as string,",
				"          Region as string,",
				"          Status as string,",
				"          Function as string,",
				"          Level as string,",
				"          Role as string,",
				"          StartDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmployeeCSV",
				"source(output(",
				"          EMPID as integer,",
				"          surrogatekey as string,",
				"          Region as string,",
				"          Status as string,",
				"          EmpFunction as string,",
				"          Level as string,",
				"          Role as string,",
				"          StartDate as date,",
				"          EndDate as date,",
				"          iscurrent as integer,",
				"          processtime as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> DimEmp",
				"EmployeeCSV filter(!isNull(EmpID)) ~> NonNullEmployeeIDRecords",
				"NonNullEmployeeIDRecords derive(EmpID = toInteger(EmpID),",
				"          processtime = currentTimestamp()) ~> TypeConversionTimestamp",
				"DimEmp derive(EMPID = toInteger(EMPID)) ~> Typecpnversions",
				"TypeConversionTimestamp, Typecpnversions lookup(TypeConversionTimestamp@EmpID == Typecpnversions@EMPID,",
				"     multiple: true,",
				"     broadcast: 'auto')~> LkpTarget",
				"LkpTarget select(mapColumn(",
				"          EmpID = TypeConversionTimestamp@EmpID,",
				"          Region = EmployeeCSV@Region,",
				"          Status = EmployeeCSV@Status,",
				"          Function,",
				"          Level = EmployeeCSV@Level,",
				"          Role = EmployeeCSV@Role,",
				"          StartDate = EmployeeCSV@StartDate,",
				"          EndDate = EmployeeCSV@EndDate,",
				"          processtime = TypeConversionTimestamp@processtime,",
				"          EMPID = Typecpnversions@EMPID,",
				"          surrogatekey,",
				"          EmpFunction,",
				"          iscurrent,",
				"          processtime = DimEmp@processtime",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> selectnames"
			]
		}
	}
}